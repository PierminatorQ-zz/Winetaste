<p id="notice"><%= notice %></p>

<h1>Winestrains</h1>

<table>
  <thead>
    <tr>
      <th>Wine</th>
      <th>Strain</th>
      <th>Percentage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @winestrains.each do |winestrain| %>
      <tr>
        <td><%= winestrain.wine %></td>
        <td><%= winestrain.strain %></td>
        <td><%= winestrain.percentage %></td>
        <td><%= link_to 'Show', winestrain %></td>
        <td><%= link_to 'Edit', edit_winestrain_path(winestrain) %></td>
        <td><%= link_to 'Destroy', winestrain, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Winestrain', new_winestrain_path %>
